mixin itemProd(a, i, t, vz, height, width, ...items)
	+b.itemProd
		if a
			+e.availability В наличии
		else
			+e.availability--empty В наличии
		+e.imgWrap
			img(src="#" data-lazy=`img/@2x/flowerpot-${i}.png`, alt="_____")
		.h4= t
			if vz
				span= vz  
			else
				span вазон бетонный 
		table
			tr
				td Высота: 
				if height
					td= height
				else
					td 300 мм
			tr
				td Длина: 
				if width
					td= width
				else
					td 400 мм
			tr
				td Вес: 
				if weight
					td= weight
				else
					td 45 кг
			block
		+e.materials
			each item in items
				+e.materialsWrap
					a(href=`img/@2x/material-${item}.jpg` data-fancybox=`materials-${i}`)
						img.res-i(src="#" data-lazy=`img/@0.5x/material-${item}.jpg` alt="фактура")

		.h5.text-center 1800 руб.*
		+e.A.btn.btn.link-modal(href="#modalOrder") 
			span.bg Заказать




